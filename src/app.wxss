/**app.wxss**/
view, text {
  font-family: PingFangSC-Medium;
}
page {
  height: 100%;
}

image {
  width: auto;
  height: auto;
  border: none;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.borderTop {
  border-top: 1px solid #ccc;
}

.bdr-bottom {
border-bottom: 1rpx solid #eee;
}
.bdr-top {
  border-top: 1rpx solid #eee;
}
.fs10 {
  font-size: 20rpx;
}
.fs12 {
  font-size: 24rpx;
}
.fs16 {
  font-size: 32rpx;
}
.col333 {
  color: #333;
}
.col666 {
  color: #666;
}
.col999 {
  color: #999;
}
button {
  border: none;
  background: #A4DEDC;
  border-radius: 24px;
  height: 48*2rpx;
  color: #fff;
  line-height: 48*2rpx;
  outline: none;
  margin-top: 32rpx;
  font-size: 32rpx;

}
button::after {
border: none;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

